                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH VehicleTypeMapping AS (
    -- Your original parsing logic as a CTE
    SELECT DISTINCT
        vehicle_type AS original_vehicle_type,
        LTRIM(
            CASE 
                WHEN CHARINDEX('(', 
                    LTRIM(SUBSTRING(vehicle_type, CHARINDEX('-', vehicle_type) + 1, LEN(vehicle_type)))
                ) &gt; 0 
                THEN LEFT(
                    LTRIM(SUBSTRING(vehicle_type, CHARINDEX('-', vehicle_type) + 1, LEN(vehicle_type))),
                    CHARINDEX('(', LTRIM(SUBSTRING(vehicle_type, CHARINDEX('-', vehicle_type) + 1, LEN(vehicle_type)))) - 1
                )
                ELSE LTRIM(SUBSTRING(vehicle_type, CHARINDEX('-', vehicle_type) + 1, LEN(vehicle_type)))
            END
        ) AS parsed_type_name
    FROM Bronze.Violation
    WHERE vehicle_type IS NOT NULL AND CHARINDEX('-', vehicle_type) &gt; 0
),
VehicleTypeWithID AS (
    SELECT 
        original_vehicle_type,
        parsed_type_name,
        ROW_NUMBER() OVER (ORDER BY parsed_type_name) AS vehicle_type_id
    FROM (
        SELECT DISTINCT parsed_type_name, original_vehicle_type 
        FROM VehicleTypeMapping
    ) AS unique_types
)
select
    v.id, 
    v.datetime_of_stop, 
    v.city,
    ve.vehicle_id,
    d.driver_type_id, 
    v.longitude, 
    v.latitude, 
    v.description, 
    v.location, 
    v.state, 
    v.violation_type,
    v.arrest_type,
    vo.violation_outcome_id,
    vd.violation_detail_id, 
    sa.sub_agency_id
from Bronze.Violation v
-- Join to get the vehicle_type_id
left join VehicleTypeWithID vt on v.vehicle_type = vt.original_vehicle_type
left join NDS.Violation_Outcome vo on vo.violation_id = v.id
left join NDS.Violation_Detail vd on vd.violation_id = v.id
left join NDS.City ci1 on v.city = ci1.city_name
left join NDS.SubAgency sa on sa.sub_agency_name = v.sub_agency and sa.city_id = ci1.city_id
left join NDS.Vehicle ve on v.year = ve.year 
    and v.make = ve.make 
    and v.model = ve.model 
    and v.color = ve.color 
    and v.commercial_vehicle = ve.commercial_vehicle
    and ve.vehicle_type_id = vt.vehicle_type_id  -- This is the key addition
left join NDS.City ci on ci.city_name = v.driver_city
left join NDS.Driver_Type d on v.race = d.race 
    and v.gender = d.gender 
    and ci.city_id = d.driver_city_id 
    and v.driver_state = d.driver_state 
    and v.commercial_license = d.commercial_license</property>
